{
  "name": "brooklyn",
  "version": "1.0.0",
  "description": "project brooklyn",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev:start": "nodemon --watch dist --exec node dist/src/index.js",
    "dev:migrate": "prisma migrate dev",
    "dev:build": "swc src -d dist --watch --source-maps",
    "format:schemas": "prisma format",
    "build:client": "prisma generate",
    "build:source": "swc src -d dist --source-maps",
    "start:node": "PREBUILT_VERSION=true node index.mjs",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org altadena --project girae ./dist && sentry-cli sourcemaps upload --org altadena --project girae ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altadenagroup/brooklyn.git"
  },
  "author": "lyricalsoul <lyricalsoul@proton.me>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/altadenagroup/brooklyn/issues"
  },
  "homepage": "https://github.com/altadenagroup/brooklyn#readme",
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "@types/node": "^20.11.28",
    "nodemon": "^3.1.0",
    "prisma": "^5.11.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@prisma/client": "^5.11.0",
    "@sentry/cli": "^2.30.2",
    "@sentry/node": "^7.107.0",
    "@sentry/profiling-node": "^7.107.0",
    "@telegraf/session": "^2.0.0-beta.7",
    "@types/graphql-fields": "^1.3.9",
    "cloudinary": "^2.0.3",
    "cron": "^3.1.6",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.5",
    "melchior": "^0.5.2",
    "openai": "^4.29.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "telegraf": "^4.16.3",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.27.2",
    "yaml": "^2.4.1"
  }
}
